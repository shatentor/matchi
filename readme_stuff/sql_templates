CREATE TABLE users (
    name VARCHAR(30),
    age INT(11),
    city VARCHAR(30),
    gender VARCHAR(10),
    photo_link VARCHAR(100),
    tg_username VARCHAR(50) NOT NULL,
    tg_chat_id VARCHAR(20) NOT NULL,
    preferred_gender VARCHAR(20),
    photo_link_two VARCHAR(200),
    photo_link_three VARCHAR(200),
    support_time VARCHAR(30),
    is_registered VARCHAR(10),
    age_lower_point INT(11),
    age_high_point INT(11),
    last_shown_profile VARCHAR(20)
);

CREATE TABLE descriptions (
    tg_chat_id VARCHAR(20) NOT NULL,
    descr VARCHAR(1000)
);
 CREATE TABLE stickers (
    lovely VARCHAR(100) NOT NULL
);

CREATE TABLE messages (
    id INT(11) NOT NULL PRIMARY KEY AUTO_INCREMENT
);

CREATE TABLE complains (
    id INT(11) NOT NULL PRIMARY KEY AUTO_INCREMENT
);

CREATE TABLE likes (
    id INT(11) NOT NULL PRIMARY KEY AUTO_INCREMENT
);

CREATE TABLE dislikes (
    id INT(11) NOT NULL PRIMARY KEY AUTO_INCREMENT
);
